<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>muto_listener</name>
  <version>0.42.0</version>
  <description>Eclipse Muto sample listener node that subscribes to string messages to demonstrate ROS 2 pub-sub communication</description>
  <maintainer email="info@composiv.ai">Composiv</maintainer>
  <license>Eclipse Public License v2.0</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
